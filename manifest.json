{
  "name": "Omni: Dark Theme for Chrome",
  "version": "1.0.4",
  "description": "The official Omni Theme for Google Chrome. Dark, clean and minimal.",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "theme": {
    "images": {
      "theme_toolbar": "images/theme_toolbar.png",
      "theme_tab_background": "images/theme_tab_background.png"
    },
    "colors": {
      "frame": [ 15, 13, 21 ],
      "frame_inactive": [ 29, 24, 42 ],
      "tab_background_text": [ 210, 210, 200 ],
      "toolbar": [ 41, 36, 62 ],
      "button_background": [ 18, 17, 27, 1 ],
      "bookmark_text": [ 240, 240, 230 ],
      "ntp_background": [ 31, 28, 45 ],
      "tab_text": [ 240, 240, 230 ],
      "ntp_text": [ 240, 240, 230 ],
      "ntp_link": [ 240, 240, 230 ]
    },
    "properties": {
      "ntp_background_alignment": "bottom",
      "ntp_background_repeat": "no-repeat"
    },
    "tints": {
      "buttons": [ 0.67, 0.09, 0.89 ]
    }
  }
}